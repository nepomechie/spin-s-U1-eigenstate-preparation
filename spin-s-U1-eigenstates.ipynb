    "# Code generated using Gemini\n",
    "def solve_bounded_composition(n, k, d):\n",
    "    m = d - 1\n",
    "    # g[0] is the buffer (g1 in Walsh's paper), g[1...n-1] are the parts\n",
    "    g = [0] * n \n",
    "\n",
    "    def get_limits(i, s_i):\n",
    "        \"\"\"\n",
    "        i: current index (0-based, but logically part i+1)\n",
    "        s_i: sum of elements to the right of index i\n",
    "        \"\"\"\n",
    "        # M_prev is the capacity of all parts to the left of index i\n",
    "        m_prev = i * m\n",
    "        l_i = max(0, k - s_i - m_prev)\n",
    "        u_i = min(m, k - s_i)\n",
    "        return l_i, u_i\n",
    "\n",
    "    # 1. Initialize the first composition\n",
    "    s = 0\n",
    "    for i in range(n - 1, 0, -1):\n",
    "        l_i, u_i = get_limits(i, s)\n",
    "        # Suffix sum 0 is even -> start at min\n",
    "        g[i] = l_i if s % 2 == 0 else u_i\n",
    "        s += g[i]\n",
    "    g[0] = k - s\n",
    "    yield list(g)\n",
    "\n",
    "    # 2. Successor Loop\n",
    "    while True:\n",
    "        pivot_i = -1\n",
    "        # To find the pivot, we check parts from left to right (excluding buffer g[0])\n",
    "        for i in range(1, n):\n",
    "            # Calculate suffix sum: sum of elements to the right of i\n",
    "            suffix_s = sum(g[i+1:])\n",
    "            l_i, u_i = get_limits(i, suffix_s)\n",
    "            \n",
    "            # Last value depends on suffix parity\n",
    "            last_val = u_i if suffix_s % 2 == 0 else l_i\n",
    "            \n",
    "            if g[i] != last_val:\n",
    "                pivot_i = i\n",
    "                current_suffix_s = suffix_s\n",
    "                break\n",
    "        \n",
    "        if pivot_i == -1:\n",
    "            break\n",
    "\n",
    "        # Move the pivot\n",
    "        if current_suffix_s % 2 == 0:\n",
    "            g[pivot_i] += 1\n",
    "        else:\n",
    "            g[pivot_i] -= 1\n",
    "            \n",
    "        # Reset all elements to the left of the pivot (but right of the buffer)\n",
    "        # to their new 'first' values based on the new suffix sum\n",
    "        new_s = sum(g[pivot_i:])\n",
    "        for j in range(pivot_i - 1, 0, -1):\n",
    "            l_j, u_j = get_limits(j, new_s)\n",
    "            g[j] = l_j if new_s % 2 == 0 else u_j\n",
    "            new_s += g[j]\n",
    "            \n",
    "        # Update buffer\n",
    "        g[0] = k - new_s\n",
    "        yield list(g)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 7,
   "id": "2cc70d69-33b1-42aa-a81a-3e8d6a835e42",
   "metadata": {},
   "outputs": [],
   "source": [
    "def Walsh(n, k, d):\n",
    "    gen=solve_bounded_composition(n, k, d)\n",
    "    res=[]\n",
    "    for x in gen:\n",
    "        res.append(x)\n",
    "    return res"
   ]
